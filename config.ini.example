[gemini]
# 联系Li_18727107073购买
api_key = YOUR_API_KEY_HERE

# Token 换算人民币调节系数（请勿调整，影响准确性）
balance_cost_factor = 0.6

# 最大并发工作线程数（建议 4-8，根据你的机器性能调整）
# 主要影响本地 CPU 侧的解码/处理并发度
max_workers = 4

# 网络并发上限（同时发起的网络请求数量上限，范围 1-8）
# 建议：
# - 稳定服务商/内网环境：4
# - 不稳定服务商/代理/VPN：2-3（降低请求雪崩概率）
network_workers_cap = 4

# 是否验证 SSL 证书（默认为 true）。
# 仅当你非常确定所连接的代理/服务安全可信、且无法正确安装根证书时，才考虑改为 false。
verify_ssl = true